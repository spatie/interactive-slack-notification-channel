{
    "name": "spatie/interactive-slack-notification-channel",
    "description": "Send interactive Slack notifications in Laravel apps",
    "keywords": [
        "spatie",
        "interactive-slack-notification-channel"
    ],
    "homepage": "https://github.com/spatie/interactive-slack-notification-channel",
    "license": "MIT",
    "authors": [
        {
            "name": "Freek Van der Herten",
            "email": "freek@spatie.be",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0",
        "guzzlehttp/guzzle": "^7.2",
        "illuminate/contracts": "^8.0",
        "illuminate/http": "^8.23",
        "illuminate/support": "^8.23"
    },
    "require-dev": {
        "ext-json": "*",
        "orchestra/testbench": "^6.0",
        "phpunit/phpunit": "^9.3",
        "spatie/ray": "^1.14"
    },
    "autoload": {
        "psr-4": {
            "Spatie\\InteractiveSlackNotificationChannel\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Spatie\\InteractiveSlackNotificationChannel\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit --colors=always",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Spatie\\InteractiveSlackNotificationChannel\\InteractiveSlackNotificationChannelServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
